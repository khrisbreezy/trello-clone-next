                            name=\"responsible\"\n                                                        placeholder=\"Responsible\"\n                                                        className=\"w-100\"\n                                                        ref={register()}\n                                                        cols=\"30\" rows=\"3\"\n                                                        />\n                                                        {/* <input type=\"text\"\n                                                            name=\"responsible\"\n                                                            placeholder=\"Responsible\"\n                                                            className=\"w-100\"\n                                                            ref={register({ required: 'This field is required' })} /> */}\n                                                        {errors.responsible && <Error>{errors.responsible.message}</Error>}\n                                                    </div>\n                                                    <div className=\"col-md-6 mb-4\">\n                                                        <textarea \n                                                        name=\"output\"\n                                                        placeholder=\"Output\"\n                                                        className=\"w-100\"\n                                                        ref={register()}\n                                                        cols=\"30\" rows=\"3\"\n                                                        />\n                                                        {/* <input type=\"text\"\n                                                            name=\"output\"\n                                                            placeholder=\"Output\"\n                                                            className=\"w-100\"\n                                                            ref={register({ required: 'This field is required' })} /> */}\n                                                        {errors.output && <Error>{errors.output.message}</Error>}\n                                                    </div>\n                                                    <div className=\"col-md-6 mb-4\">\n                                                        <textarea \n                                                        name=\"notes\"\n                                                        placeholder=\"Notes\"\n                                                        className=\"w-100\"\n                                                        ref={register()}\n                                                        cols=\"30\" rows=\"3\"\n                                                        />\n                                                        {/* <input type=\"text\"\n                                                            name=\"notes\"\n                                                            placeholder=\"Notes\"\n                                                            className=\"w-100\"\n                                                            ref={register({ required: 'This field is required' })} /> */}\n                                                        {errors.notes && <Error>{errors.notes.message}</Error>}\n                                                    </div>\n                                                </div>\n\n                                                <div className=\"text-center\">\n                                                        <button disabled={loading} type=\"submit\" className=\"btn btn-login\">\n                                                            {!loading ? 'Update' : 'Updating...'}\n                                                        </button>\n                                                </div>\n                                            </form>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </>\n    )\n};\n\nexport default AuthModal;"]},"metadata":{},"sourceType":"module"}